# -- Loading package STANDARD
# -- Loading package TEXTIO
# -- Loading package std_logic_1164
# -- Compiling entity Vhdl3
# -- Compiling architecture rtl of Vhdl3
# 
vsim -voptargs=+acc work.vhdl3
# vsim -voptargs=+acc work.vhdl3 
# Loading std.standard
# Loading std.textio(body)
# Loading ieee.std_logic_1164(body)
# Loading work.vhdl3(rtl)
add wave -position insertpoint  \
sim:/vhdl3/nrst
wave create -driver freeze -pattern constant -value 1 -starttime 0ps -endtime 1000ps sim:/vhdl3/nrst
add wave -position insertpoint  \
sim:/vhdl3/clk
force -freeze sim:/vhdl3/clk 1 0, 0 {50 ps} -r 100
wave create -driver freeze -pattern clock -initialvalue 0 -period 100ps -dutycycle 50 -starttime 0ps -endtime 1000ps sim:/vhdl3/clk
run
add wave -position insertpoint  \
sim:/vhdl3/clk_out
run
run
run
run
run
run
run
run
run
run
run
