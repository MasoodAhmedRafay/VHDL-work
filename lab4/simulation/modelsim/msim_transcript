# -- Compiling entity lab4
# -- Compiling architecture behavior of lab4
# 
add wave -position end  sim:/lab4/shift_in
add wave -position end  sim:/lab4/shift_out
force -freeze sim:/lab4/rst 1 0
add wave -position insertpoint  \
sim:/lab4/rst
add wave -position insertpoint  \
sim:/lab4/clk
force -freeze sim:/lab4/clk 1 0, 0 {50 ps} -r 100
wave create -driver freeze -pattern random -initialvalue 1 -period 50ps -random_type Normal -seed 5 -starttime 0ps -endtime 1000ps sim:/lab4/shift_in
force -freeze sim:/lab4/clk 1 0, 0 {50 ps} -r 100
force -freeze sim:/lab4/clk 1 0, 0 {50 ps} -r 100
add wave -position end  sim:/lab4/clk
force -freeze sim:/lab4/clk 1 0, 0 {50 ps} -r 100
force -freeze sim:/lab4/clk 1 0, 0 {50 ps} -r 100
wave create -driver freeze -pattern clock -initialvalue 1 -period 100ps -dutycycle 50 -starttime 0ps -endtime 1000ps sim:/lab4/clk
force -freeze sim:/lab4/rst 1 0
wave create -driver freeze -pattern constant -value 0 -starttime 0ps -endtime 1000ps sim:/lab4/rst
wave create -driver expectedOutput -pattern constant -value UUUUUUUUUUUUUUUU -range 15 0 -starttime 0ps -endtime 1000ps sim:/lab4/shift_out
run
run
run
run
run
run
run -continue
run -all
run -continue
